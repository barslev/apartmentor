{
  "name": "apartmentor",
  "version": "0.2.0",
  "description": "Search for apartments on Studentbostäder",
  "author": "Max Davidson <davidsonmax@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/maxdavidson/apartmentor.git"
  },
  "main": "dist/index.js",
  "bin": "dist/cli.js",
  "typings": "dist/index.d.ts",
  "engineStrict": true,
  "engines": {
    "node": ">=6.4.0"
  },
  "scripts": {
    "build": "tsc",
    "test": "nyc --reporter=text --reporter=html ava",
    "clean": "rm -rf coverage dist",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "prebuild": "npm run clean",
    "pretest": "npm run build",
    "preversion": "npm test",
    "postversion": "git push && git push --tags && npm publish"
  },
  "keywords": [
    "studentbostäder",
    "linköping",
    "api"
  ],
  "dependencies": {
    "flat-cache": "^1.2.1",
    "goo.gl": "^0.1.4",
    "node-fetch": "^1.6.0",
    "yargs": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^6.0.37",
    "@types/node-fetch": "0.0.2",
    "@types/yargs": "0.0.29",
    "ava": "^0.16.0",
    "nyc": "^8.1.0",
    "typescript": "^2.0.0"
  }
}
